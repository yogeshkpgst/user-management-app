<h3>Welcome</h3>  

<% if user_signed_in? %>
<h3>Hi, <%= current_user.name %></h3>

 <div class="row">
   <div class="col-sm-6">
     <div id="chart-1" style=" width: 100px height: 100px;"></div>
       <%= bar_chart({"You" => 24.10, "Efficient Neighbor" => 7.90, "All Neighbor" => 10.20}) %>
     </div>
   </div>
 </div>


<div class="row">
   <div class="col-sm-6">
     <div id="chart-2" style=" width: 100px height: 100px;"></div>
       <%= bar_chart({"You" => 74.10, "Efficient Neighbor" => 87.90, "All Neighbor" => 60.20}) %>
     </div>
   </div>
 </div>

 <% else %>
  <%= content_tag(flash[:error] = "Login to access this page") %>
<% end %>