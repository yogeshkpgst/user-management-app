<div class="authform" >
  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :role => 'form'}) do |f| %>
  <div style = "height:850px">
    <h3>Personal Information</h3> 
    <%= devise_error_messages! %>

    <div>
      <%= f.label :account_no %>
      <%= f.text_field :ac_no, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="row" style="display:inline" >

    <div class="form-group col-xs-6">
      <%= f.label :prefix %>
      <%= f.select :prefix, options_for_select(%w[Dr. Prof. Er. Mr. Mrs. Ms.]) , {:class => 'date-select'} %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :street1 %>
      <%= f.text_field :street1, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :firstname , class:'required' %>
      <%= f.text_field :name, :autofocus => true, class: 'form-control' %>
   </div>

    <div class="form-group col-xs-6">
      <%= f.label :street2 %>
      <%= f.text_field :street2, :autofocus => true, class: 'form-control' %>
    </div>

   <div class="form-group col-xs-6">
      <%= f.label :lastname , class:'required' %>
      <%= f.text_field :last_name, :autofocus => true, class: 'form-control' %>
   </div>

    <div class="form-group col-xs-6">
      <%= f.label :city %>
      <%= f.text_field :city, :autofocus => true, class: 'form-control' %>
    </div>
    
    <div class="form-group col-xs-6">
      <%= f.label :suffix %>
      <%= f.text_field :suffix, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :state %>
      <%= f.text_field :state, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :title %>
      <%= f.text_field :title, :autofocus => true, class: 'form-control' %>
    </div>

   <div class="form-group col-xs-6">
      <%= f.label :country %>
      <%= f.text_field :country, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :email , class:'required' %>
      <%= f.email_field :email, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :zip %>
      <%= f.text_field :zip, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :password , class:'required' %>
      <%= f.password_field :password, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :phone %>
      <%= f.text_field :phone, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :password_confirmation, class:'required' %>
      <%= f.password_field :password_confirmation, class: 'form-control' %>
    </div>
    
    <div class="form-group col-xs-6">
      <%= f.label :fax %>
      <%= f.text_field :fax, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :nickname %>
      <%= f.text_field :nick_name, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :group %>
      <%= f.text_field :group, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :rate_code %>
      <%= f.text_field :rate_code, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group col-xs-6">
      <%= f.label :ac_type %>
      <%= f.text_field :ac_type, :autofocus => true, class: 'form-control' %>
    </div>
    
     <%= f.submit 'Sign up', :class => 'button right' %>
  <% end %>
</div>
</div>
